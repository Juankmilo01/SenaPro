@model IEnumerable<Microsoft.AspNet.Identity.EntityFramework.IdentityRole>

@{
    ViewBag.Title = "Index";
}
<br />
<br />
<p>
    @Html.ActionLink("Registrar Rol", "Create", null, new { @class = "btn btn-primary" })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>

        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.ActionLink("Editar", "Edit", null, new { id = item.Id }, new { @class = "btn btn-info" }) |
                @*@Html.ActionLink("Detalles", "Details",null, new { id=item.Id }, new { @class = "btn btn-success" }) |*@
                @Html.ActionLink("Eliminar", "Delete", null, new { id = item.Id, }, new { @class = "btn btn-danger" })
            </td>
        </tr>
    }

</table>
